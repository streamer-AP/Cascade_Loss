{
    "Model": {
        "name": "csrnet"
    },
    "Dataset": {
        "train": {
            "name": "nwpu_crowd",
            "type": "train",
            "task": "counting",
            "ann_file": ".data/jhu_crowd_v2.0/train_annotation.json",
            "img_prefix": ".data/jhu_crowd_v2.0/train/images",
            "file_list": ".data/jhu_crowd_v2.0/train/image_labels.txt",
            "size": 1024,
            "img_norm_cfg": {
                "mean": [
                    0.485,
                    0.456,
                    0.406
                ],
                "std": [
                    0.229,
                    0.224,
                    0.225
                ]
            },
            "size_divisor": 32,
            "batch_size": 4,
            "num_workers": 16,
            "shuffle": true,
            "drop_last": true,
            "cache_mode": false,
            "max_len": 30000
        },
        "val": {
            "name": "nwpu_crowd",
            "type": "val",
            "task": "counting",
            "ann_file": ".data/jhu_crowd_v2.0/val_annotation.json",
            "img_prefix": ".data/jhu_crowd_v2.0/val/images",
            "file_list": ".data/jhu_crowd_v2.0/val/image_labels.txt",
            "size": 1024,
            "img_norm_cfg": {
                "mean": [
                    0.485,
                    0.456,
                    0.406
                ],
                "std": [
                    0.229,
                    0.224,
                    0.225
                ]
            },
            "size_divisor": 32,
            "batch_size": 1,
            "num_workers": 4,
            "shuffle": false,
            "drop_last": true,
            "cache_mode": false,
            "max_len": 30000
        },
        "test": {
            "name": "nwpu_crowd",
            "task": "counting",

            "type": "test",
            "ann_file": ".data/jhu_crowd_v2.0/test_annotation.json",
            "file_list": ".data/jhu_crowd_v2.0/test/image_labels.txt",
            "img_prefix": ".data/jhu_crowd_v2.0/test/images",
            "size": 1024,
            "img_norm_cfg": {
                "mean": [
                    0.485,
                    0.456,
                    0.406
                ],
                "std": [
                    0.229,
                    0.224,
                    0.225
                ]
            },
            "size_divisor": 32,
            "batch_size": 1,
            "num_workers": 4,
            "shuffle": false,
            "drop_last": true,
            "cache_mode": false,
            "max_len": 30000
        }
    },
    "Optimizer": {
        "type": "Adamw",
        "lr": 0.0001,
        "betas": [
            0.9,
            0.999
        ],
        "eps": 1e-08,
        "weight_decay": 0.000001
    },
    "Loss": {
        "counting": {
            "type": "dmap_loss",
            "args": {
                "name": "dmap_loss",
                "kernel_size": 0,
                "num_factor": 1,
                "mae_weight": 0,
                "mse_weight": 1,
                "scale_mae_weight": 0,
                "casecade_mae_weight": 0,
                "casecade_mse_weight": 1,

                "all_mae_weight": 0,
                "all_rmse_weight": 0,
                "ot_weight": 0,
                "resize_to_original": false,
                "weights_kernel_size": 2,
                "smooth": false,
                "scale_list": [1, 2, 4, 8, 16, 32]
            }
        }

    },
    "Loss_Weight": {
        "type": "constant"
    },
    "Scheduler": {
        "type": "cosine",
        "T_max": 150,
        "eta_min": 0.0000001,
        "ema": false,
        "ema_annel_strategy": "cos",
        "ema_annel_epochs": 10,
        "ema_lr": 0.000000001,
        "ema_weight": 0.9,
        "ema_start_epoch": 90
    },
    "Saver": {
        "save_dir": "outputs",
        "save_interval": 10,
        "save_num_per_epoch": 2,
        "max_save_num": 20,
        "save_best": true,
        "metric": "mae"
    },
    "Logger": {
        "delimiter": "\t",
        "print_freq": 50,
        "header": ""
    },
    "Misc": {
        "use_gpu": true,
        "print_freq": 1,
        "epochs": 500,
        "clip_max_norm": 10,
        "resume": false,
        "resume_path": "",
        "load_from_best": false,
        "start_epoch": 0,
        "best_mAP": 0,
        "workers": 4,
        "use_tensorboard": true,
        "tensorboard_dir": "outputs/tensorboard",
        "use_visdom": false,
        "visdom_port": 8097,
        "visdom_env": "main",
        "visdom_server": "http://localhost",
        "use_checkpoint": true,
        "checkpoint_dir": "outputs/checkpoints",
        "checkpoint_interval": 1,
        "checkpoint_num_per_epoch": 2,
        "max_checkpoint_num": 20
    },
    "Drawer": {
        "draw_freq": 50,
        "output_dir": "outputs/vis",
        "draw_original": false,
        "draw_denseMap": false,
        "draw_output": true,
        "draw_point": true,
        "mean": [
            0.485,
            0.456,
            0.406
        ],
        "std": [
            0.229,
            0.224,
            0.225
        ]
    }
}